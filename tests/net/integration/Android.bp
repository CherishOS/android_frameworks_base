// Utilities for testing framework code both in integration and unit tests.
java_library {
    name: "frameworks-net-integration-testutils",
    srcs: ["util/**/*.java", "util/**/*.kt"],
    static_libs: [
        "androidx.annotation_annotation",
        "androidx.test.rules",
        "junit",
        "net-tests-utils",
    ],
    libs: [
        "services.core",
        "services.net",
    ],
}
