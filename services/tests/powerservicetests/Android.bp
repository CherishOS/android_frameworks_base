package {
    // See: http://go/android-license-faq
    default_applicable_licenses: ["frameworks_base_license"],
}

android_test {
    name: "PowerServiceTests",

    srcs: [
        "src/**/*.java",
    ],

    static_libs: [
        "frameworks-base-testutils",
        "platform-test-annotations",
        "services.core",
        "servicestests-utils",
    ],

    platform_apis: true,
    test_suites: [
        "device-tests",
        "automotive-tests",
    ],

    certificate: "platform",

    dxflags: ["--multi-dex"],

    optimize: {
        enabled: false,
    },
}
